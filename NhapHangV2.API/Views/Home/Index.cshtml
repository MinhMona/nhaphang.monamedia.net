        <script>
            const transectionInputElm = document.querySelector("#transection-input");
            const transectionButtonElm = document.querySelector("#transection-button");
            const closeBtnElm = document.querySelector(".popup.iframe .btn.popup-button-close");
            const popupIframeElm = document.querySelector(".popup.iframe");
            const titleElm = document.querySelector(".popup.iframe .popup-title span");

            if (transectionButtonElm && transectionInputElm) {
                transectionButtonElm.addEventListener("click", (e) => {
                    e.preventDefault();
                    titleElm.innerHTML = transectionInputElm.value;
                    const newHerf = `/tracking/code/?TransactionCode=${transectionInputElm.value}`

                    if (popupIframeElm) {
                        document.querySelector('body').classList.add('no-scroll')
                        popupIframeElm.querySelector(".popup-content iframe").setAttribute('src', newHerf)
                        popupIframeElm.classList.add('open');
                    }
                })
            }

            if (closeBtnElm) {
                closeBtnElm.addEventListener('click', (e) => {
                    e.preventDefault();
                    popupIframeElm.classList.remove('open');
                    document.querySelector('body').classList.remove('no-scroll')

                })
            }
        </script>

