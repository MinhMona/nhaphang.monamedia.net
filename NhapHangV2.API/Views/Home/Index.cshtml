        <section class="sec sec-register">
            <div class="container">
                <div class="sec-register-text">
                    <h3 class="sub-title" data-aos="fade-up" data-aos-delay="100">
                        hướng dẫn
                    </h3>
                    <h1 class="main-title" data-aos="fade-up" data-aos-delay="200">
                        Đăng ký Sử Dụng
                    </h1>
                </div>

                <div class="sec-register-wrapper">
                    <div class="sec-register-step">
                        @{
                            timeOut = 0; int stepCount = 0;
                        }
                        @foreach (var item in Model.StepList)
                        {
                            timeOut += 100;
                            stepCount++;
                            <div class="sec-register-step-item">
                                <div class="sec-register-step-item-number"
                                     data-aos="fade-down"
                                     data-aos-delay="@timeOut">
                                    <span>@stepCount</span>
                                </div>
                                <div class="sec-register-step-item-info"
                                     data-aos="fade-up"
                                     data-aos-delay="@timeOut">
                                    <div class="sec-register-step-item-title">
                                        @item.Name
                                    </div>
                                    <div class="sec-register-step-item-dot active"></div>
                                </div>
                            </div>
                        }

                        <div class="sec-register-step-line"></div>
                        <div class="sec-register-step-lineactive"></div>
                    </div>

                    <div class="sec-register-slide">
                        <div class="sec-register-slide-count">Bước: <span>1</span></div>
                        <div class="sec-register-slide-textWrapper swiper swiper-textWrapper"
                             data-aos="zoom-in-right">
                            <div class="swiper-wrapper">
                                <!-- Slides -->
                                @foreach (var item in Model.StepList)
                                {
                                    <div class="swiper-slide">
                                        <div class="sec-register-slide-textWrapper-item">
                                            <h2>@item.Name</h2>
                                            <p>
                                                @item.Description
                                            </p>
                                        </div>
                                    </div>
                                }

                            </div>
                            <div class="sec-register-slide-textWrapper-navigation">
                                <div class="sec-register-slide-textWrapper-prev"
                                     data-aos="fade-up"
                                     data-aos-delay="200">
                                    <img src="./demo-nhtq/assets/images/register-prev-icon.webp" alt="" />
                                </div>
                                <div class="sec-register-slide-textWrapper-next"
                                     data-aos="fade-up"
                                     data-aos-delay="400">
                                    <img src="./demo-nhtq/assets/images/register-next-icon.webp" alt="" />
                                </div>
                            </div>
                        </div>

                        <div class="sec-register-slide-imageWrapper"
                             data-aos="zoom-in-left">
                            <img src="./demo-nhtq/assets/images/register-image-1.webp" alt="" />
                        </div>
                    </div>
                </div>
            </div>
