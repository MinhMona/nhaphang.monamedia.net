                    <h1 class="main-title" data-aos="fade-up" data-aos-delay="200">
                        Bạn có nhu cầu
                    </h1>
                    <h2 class="sec-service-hightligh">Chúng tôi có Dịch Vụ</h2>
                </div>

                <div class="sec-service-main">
                    <div class="sec-service-left">
                        <ul class="sec-service-list">
                            @{
                                int timeOut = 0;
                            }
                            @for (int i = 0; i < 3; i++)
                            {
                                timeOut += 100;
                                <li class="sec-service-item"
                                    data-aos="zoom-in-left"
                                    data-aos-delay="@timeOut">
                                    <img src="@Model.ServiceList[i].IMG" alt="" />
                                    <div class="sec-service-main-text">
                                        <h3>@Model.ServiceList[i].Name</h3>
                                        <p>
                                            @Model.ServiceList[i].Description
                                        </p>
                                    </div>
                                </li>
                            }
                        </ul>
                    </div>
                    <img src="./demo-nhtq/assets/images/center-service.webp"
                         alt=""
                         class="sec-service-img"
                         data-aos="zoom-in-up"
                         data-aos-delay="100" />
                    <div class="sec-service-right">
                        <ul class="sec-service-list">
                            @{
                                timeOut = 200;
                            }
                            @for (int i = 3; i < 6; i++)
                            {
                                timeOut += 100;
                                <li class="sec-service-item"
                                    data-aos="zoom-in-right"
                                    data-aos-delay="@timeOut">
                                    <img src="@Model.ServiceList[i].IMG" alt="" />
                                    <div class="sec-service-main-text">
                                        <h3>@Model.ServiceList[i].Name</h3>
                                        <p>
