@{

    var config = Model.Configurations;
    var user = Model.UserData;
}
<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta property="og:title" content="@config.OGTitle" />
    <meta property="og:type" content="website" />
    <meta property="og:image" content="@config.OGImage" />
    <meta property="og:description" content="@config.OGDescription" />
    <meta name="title" content="@config.WebsiteName" />
    <meta name="desciption" content="@config.OGDescription" />

    <title>Home</title>
    <link rel="shortcut icon" href="./CSSLEPHUONGNEW/assets/images/favicon/fav.png" type="image/x-icon" />
    <link rel="stylesheet" href="./CSSLEPHUONGNEW/css/style.css" />
